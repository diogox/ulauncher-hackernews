{
    "manifest_version": "1",
    "api_version": "1",
    "name": "HackerNews",
    "description": "Read the top stories on HackerNews",
    "developer_name": "Diogo Xavier",
    "icon": "images/hackernews.png",
    "options": {
        "query_debounce": 1
    },
    "preferences": [
        {
            "id": "keyword",
            "type": "keyword",
            "name": "Keyword",
            "default_value": "hn"
        },
        {
            "id": "open_mode",
            "type": "select",
            "name": "Open Article's",
            "options": ["Comment Section", "URL"],
            "default_value": "Comment Section"
        }
    ]
}